<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate='1'>
		<!-- Sequences for Car Vue Job -->
		<record id='seq_type_job' model='ir.sequence.type'>
			<field name='name'>Job Sequence</field>
			<field name='code'>job.order</field>			
		</record>

		<record id='seq_job' model='ir.sequence'>
			<field name='name'>Job Sequence</field>
			<field name='code'>job.order</field>
			<field name='prefix'>JOB</field>
			<field name='padding'>5</field>
		</record>

		<!-- Sequences for Car Vue Vehicles -->
		<record id='seq_type_vehicle' model='ir.sequence.type'>
			<field name='name'>Vehicle Sequence</field>
			<field name='code'>vehicle.dashboard</field>
		</record>

		<record id='seq_vehicle' model='ir.sequence'>
			<field name='name'>Vehicle Sequence</field>
			<field name='code'>vehicle.dashboard</field>
			<field name='prefix'>VEH</field>
			<field name='padding'>5</field>
		</record>
		
	</data>
</openerp>